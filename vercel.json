{
    "version": 2,
    "builds": [
        {
            "src": "./main.py",
            "use": "@vercel/python"
        },
        {
          "src": "/static/",
          "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app/main.py"
        },
        {
          "src": "/api/(.*)",
          "dest": "/app/main.py"
        }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "https://github.com/JBYT27/replit-readme-stats"
      }
    ]
}
