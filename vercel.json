{
    "version": 2,
    "builds": [
        {
            "src": "./main.py",
            "use": "@vercel/python"
        },
        {
          "src": "/static/",
          "use": "@now/static"
        }
    ],
    "routes": [
        {
          "src": "/api", 
          "methods": ["POST"],
          "dest": "/main.py" 
        },
        {
          "src": "/api/(?<username>[^/]*)&(?<style>[^/]*)",
          "dest": "/main.py?username=$username&style=$style"
        },
        {
            "src": "/",
            "dest": "/main.py"
        }
    ]
}
