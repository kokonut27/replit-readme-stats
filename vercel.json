{
    "version": 2,
    "builds": [
        {
            "src": "./main.py",
            "use": "@vercel/python"
        },
        {
          "src": "/static/",
          "use": "@now/static"
        }
    ],
    "routes": [
        {
          "src": "/api/",
          "dest": "/templates/user.html"
        },
        {
            "src": "/",
            "dest": "./main.py"
        }
    ]
}
